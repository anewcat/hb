{"ast":null,"code":"export default {\n  data() {\n    return {\n      musicArr: [{\n        id: 1,\n        detail: '铄姐很想让我唱但我还没唱的歌',\n        url: require('../../../public/music/1namejiaoao.mp3')\n      }, {\n        id: 2,\n        detail: '听点高兴的',\n        url: require('../../../public/music/2hongshanguo.mp3')\n      }, {\n        id: 3,\n        detail: '你相信吗 也许在另一个平行时空 有一个跟你长得很像很像的人 正在热烈得爱着那个你爱不到的人',\n        url: require('../../../public/music/3xiangjianni.mp3')\n      }, {\n        id: 4,\n        detail: '永远爱你是我说过~',\n        url: require('../../../public/music/4womenlia.mp3')\n      }, {\n        id: 5,\n        detail: '不知道啥歌，在铄姐网易云歌单看见的日本歌',\n        url: require('../../../public/music/5ribenge.mp3')\n      }, {\n        id: 6,\n        detail: '铄姐最喜欢的风格之一',\n        url: require('../../../public/music/6fengyucaihong.mp3')\n      }, {\n        id: 7,\n        detail: '铄姐墙裂推荐',\n        url: require('../../../public/music/7yiqiyihui.mp3')\n      }, {\n        id: 8,\n        detail: '世界上有很多很多很多很多很多的人~',\n        url: require('../../../public/music/8nameduoren.mp3')\n      }],\n      count: [0, 0, 0, 0, 0, 0, 0, 0],\n      isTimerPlaying: false,\n      audio: '',\n      str: '点图标可以听到音乐哦~'\n    };\n  },\n  created() {\n    // console.log(this.audio,\"111\")\n    if (!this.audio) {\n      this.audio = new Audio();\n      this.audio.loop = true;\n      // console.log(this.audio,\"222\")\n    }\n  },\n\n  methods: {\n    countAdd(id, url) {\n      if (this.count[id - 1] < 10) {\n        this.count[id - 1]++;\n        switch (this.count[id - 1]) {\n          case 1:\n            this.str = '让你点你就点？';\n            break;\n          case 2:\n            this.str = '你再点一个试试？';\n            break;\n          case 3:\n            this.str = '呦呵~再点一下';\n            break;\n          case 4:\n            this.str = '可以~';\n            break;\n          case 5:\n            this.str = '再坚持一下！！！';\n            break;\n          case 6:\n            this.str = '就快听到了！';\n            break;\n          case 7:\n            this.str = '@嘿嘿，逗你玩，还要接着点@';\n            break;\n          case 8:\n            this.str = '这次是真的快听到了哦~~';\n            break;\n          case 9:\n            this.str = '马上了马上了！！！';\n            break;\n        }\n      }\n      if (this.count[id - 1] >= 10) {\n        this.str = '再点点别的试试~';\n        this.play(url);\n      }\n    },\n    play(myurl) {\n      // console.log(this.audio.src,myurl)\n      var res = this.audio.src.split('/');\n      var thisurl = '/' + res[res.length - 2] + '/' + res[res.length - 1];\n      console.log(thisurl, myurl);\n      if (this.audio.src == '') {\n        this.audio.src = myurl;\n        if (this.audio.paused) {\n          this.audio.play();\n        } else {\n          this.audio.pause();\n        }\n      } else if (thisurl === myurl) {\n        if (this.audio.paused) {\n          this.audio.play();\n        } else {\n          this.audio.pause();\n        }\n      } else {\n        this.audio.src = myurl;\n        this.audio.play();\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA2BA;EACAA;IACA;MACAC;QACAC;QACAC;QACAC;MAEA,GACA;QACAF;QACAC;QACAC;MACA,GACA;QACAF;QACAC;QACAC;MACA,GACA;QACAF;QACAC;QACAC;MACA,GACA;QACAF;QACAC;QACAC;MACA,GACA;QACAF;QACAC;QACAC;MACA,GACA;QACAF;QACAC;QACAC;MACA,GACA;QACAF;QACAC;QACAC;MACA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;MACA;MACA;MACA;IACA;EAEA;;EACAC;IACAC;MACA;QACA;QACA;UAEA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;UACA;YACA;YACA;QAAA;MAGA;MACA;QACA;QACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACAC;MACA;QACA;QACA;UACA;QACA;UACA;QACA;MACA;QACA;UACA;QACA;UACA;QACA;MACA;QACA;QACA;MACA;IACA;EACA;AAEA","names":["data","musicArr","id","detail","url","count","isTimerPlaying","audio","str","created","methods","countAdd","play","console"],"sourceRoot":"src/pages/Music","sources":["index.vue"],"sourcesContent":["<template>\r\n<div class=\"root\">\r\n    <div class